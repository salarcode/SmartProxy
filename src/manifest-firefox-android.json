{
	"name": "__MSG_extensionName__",
	"version": "1.4.1",
	"manifest_version": 2,
	"author": "Salar Khalilzadeh",
	"description": "__MSG_extensionDescription__",
	"homepage_url": "https://github.com/salarcode/SmartProxy",
	"default_locale": "en",
	"icons": {
		"16": "icons/smartproxy-16.png",
		"24": "icons/smartproxy-24.png",
		"48": "icons/smartproxy-48.png",
		"96": "icons/smartproxy-96.png",
		"128": "icons/smartproxy-128.png"
	},
	"permissions": [
		"<all_urls>",
		"activeTab",
		"tabs",
		"proxy",
		"webRequest",
		"webRequestBlocking",
		"storage",
		"unlimitedStorage",
		"notifications"
	],
	"browser_action": {
		"browser_style": true,
		"default_title": "__MSG_extensionName__",
		"default_popup": "ui/popup.html",
		"default_icon": {
			"16": "icons/smartproxy-16.png",
			"24": "icons/smartproxy-24.png",
			"48": "icons/smartproxy-48.png",
			"96": "icons/smartproxy-96.png",
			"128": "icons/smartproxy-128.png"
		}
	},
	"background": {
		"scripts": [
			"core.js"
		]
	},
	"options_ui": {
		"page": "ui/settings.html",
		"open_in_tab": true,
		"browser_style": false
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "smartproxy@salarcode.com",
			"strict_min_version": "100.0"
		},
		"gecko_android": {
			"strict_min_version": "120.0"
		}
	}
}